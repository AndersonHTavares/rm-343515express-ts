{
  "name": "express-ts",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "^10.15.2"
  },
  "scripts": {
    "start": "bin/www",
    "dev": "nodemon --inspect",
    "copy-static": "cp -r views dist/ && cp -r public dist/",
    "ts-compile": "tsc",
    "ts-watch": "tsc -w",
    "clean": "rm -rf node_modules/ dist/",
    "start-docker": "docker build -it express-ts . && docker run -d -p 3000:3000 express-ts",
    "lint": "tslint -p ."
  },
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "http-errors": "^1.7.3",
    "morgan": "^1.9.1",
    "pug": "^2.0.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.3",
    "@types/http-errors": "^1.6.3",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.9.1",
    "nodemon": "^2.0.2",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  }
}

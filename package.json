{
  "name": "express-ts",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "^10.15.2"
  },
  "scripts": {
    "start": "bin/www",
    "dev": "nodemon --inspect",
    "copy-static": "cp -r views dist/ && cp -r public dist/",
    "ts-compile": "tsc",
    "ts-watch": "tsc -w",
    "clean": "rm -rf node_modules/ dist/",
    "start-docker": "docker build -it express-ts . && docker run -d -p 3000:3000 express-ts",
    "lint": "tslint -p ."
  },
  "dependencies": {
    "appmetrics-dash": "^4.1.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.4",
    "http-errors": "^1.7.2",
    "morgan": "^1.9.1",
    "pug": "^2.0.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/http-errors": "^1.6.1",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.10",
    "nodemon": "^1.18.6",
    "tslint": "^5.13.1",
    "typescript": "^3.3.0"
  }
}
